doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title Student
  body 
    h1 "text pug template engine with express"
    p "Student List passed by express"
    ul
      each item in stdList
        li= item.name